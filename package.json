{
  "name": "crasy",
  "description": "Create React App Template with Redux Toolkit Query, React Router and TypeScript",
  "keywords": [
    "react",
    "redux",
    "redux-toolkit",
    "rtk",
    "rtk-query",
    "pwa",
    "progressive-web-app",
    "react-router",
    "typescript"
  ],
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.5.1",
    "@reduxjs/toolkit": "^1.8.2",
    "@rtk-query/graphql-request-base-query": "^2.1.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/algoliasearch": "^4.0.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.43",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "algoliasearch": "^4.13.1",
    "get-graphql-schema": "^2.1.2",
    "graphql-request": "^4.3.0",
    "history": "^5.3.0",
    "prettier": "^2.7.0",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "redux-first-history": "^5.0.9",
    "typescript": "^4.7.3",
    "use-query-params": "^1.2.3",
    "web-vitals": "^2.1.4",
    "workbox-background-sync": "^6.5.3",
    "workbox-broadcast-update": "^6.5.3",
    "workbox-cacheable-response": "^6.5.3",
    "workbox-core": "^6.5.3",
    "workbox-expiration": "^6.5.3",
    "workbox-google-analytics": "^6.5.3",
    "workbox-navigation-preload": "^6.5.3",
    "workbox-precaching": "^6.5.3",
    "workbox-range-requests": "^6.5.3",
    "workbox-routing": "^6.5.3",
    "workbox-strategies": "^6.5.3",
    "workbox-streams": "^6.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "grapqhlzero-to-ts": "graphql-codegen --config codegen.yml",
    "grapqhlzero-to-schema": "get-graphql-schema https://graphqlzero.almansi.me/api > schema.graphql",
    "import-all": "npm run grapqhlzero-to-schema && npm run grapqhlzero-to-ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-useless-concat": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
