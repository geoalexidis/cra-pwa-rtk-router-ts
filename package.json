{
  "name": "crasy",
  "description": "Create React App Template with Redux Toolkit Query, React Router and TypeScript",
  "keywords": [
    "react",
    "redux",
    "redux-toolkit",
    "rtk",
    "rtk-query",
    "pwa",
    "progressive-web-app",
    "react-router",
    "typescript"
  ],
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/typescript": "^2.4.2",
    "@reduxjs/toolkit": "^1.7.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/algoliasearch": "^4.0.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.21",
    "@types/react-router-dom": "^5.3.2",
    "algoliasearch": "^4.11.0",
    "get-graphql-schema": "^2.1.2",
    "graphql-request": "^3.7.0",
    "history": "^5.2.0",
    "prettier": "^2.5.1",
    "query-string": "^7.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "redux-first-history": "^5.0.3",
    "typescript": "^4.5.4",
    "use-query-params": "^1.2.3",
    "web-vitals": "^2.1.4",
    "workbox-background-sync": "^6.4.2",
    "workbox-broadcast-update": "^6.4.2",
    "workbox-cacheable-response": "^6.4.2",
    "workbox-core": "^6.4.2",
    "workbox-expiration": "^6.4.2",
    "workbox-google-analytics": "^6.4.2",
    "workbox-navigation-preload": "^6.4.2",
    "workbox-precaching": "^6.4.2",
    "workbox-range-requests": "^6.4.2",
    "workbox-routing": "^6.4.2",
    "workbox-strategies": "^6.4.2",
    "workbox-streams": "^6.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "graphcms-to-ts": "graphql-codegen --config codegen.yml",
    "graphcms-to-schema": "get-graphql-schema https://graphqlzero.almansi.me/api > schema.graphql",
    "import-all": "npm run graphcms-to-schema && npm run graphcms-to-ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-useless-concat": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
